<!-- An example that shows how to do a basic install of Fedora 14
     i386 using the ISO type install from a local file.
-->
<template>
  <name>f14jeos</name>
  <os>
    <name>Fedora</name>
    <version>14</version>
    <arch>i386</arch>
    <install type='iso'>
      <iso>file:///Fedora-14-i386-netinst.iso</iso>
    </install>
  </os>
  <description>Fedora 14</description>
</template>
