<!-- An example that shows how to do a basic install of Windows 2000
     i386 using the ISO type install.  Note that Windows requires a
     key, so Oz will fail if the <key> element is missing.
-->
<template>
  <name>win2kjeos</name>
  <os>
    <name>Windows</name>
    <version>2000</version>
    <arch>i386</arch>
    <install type='iso'>
      <iso>http://path/to/isos/win2k.iso</iso>
    </install>
    <key>MY_KEY_HERE</key>
  </os>
  <description>Windows 2000</description>
</template>
